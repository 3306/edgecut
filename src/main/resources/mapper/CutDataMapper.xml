<mapper namespace="com.edgecut.mapper.CutDataMapper">

    <sql id="allFields">
        id, `key`, prefix, status, x, y, w, h,
        gmt_create as gmtCreate, gmt_modified as gmtModified, delete_mark as deleteMark
    </sql>

